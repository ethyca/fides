{
  "name": "admin-ui",
  "private": true,
  "scripts": {
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "build": "next build",
    "chrome:debug": "BROWSER='google chrome' BROWSER_ARGS='--remote-debugging-port=9222' npm run dev",
    "copy-export": "mkdir -p ../../src/fides/ui-build/static/admin/ && rsync -a --delete out/ ../../src/fides/ui-build/static/admin/",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:start": "NODE_ENV=test next build && NODE_ENV=test next start",
    "dev": "next dev",
    "dev:mock": "echo 'ðŸš¨ Running with mock API'; NEXT_PUBLIC_MOCK_API=true next dev",
    "export": "next build && next export",
    "prod-export": "npm run export && npm run copy-export",
    "format": "prettier --write src __tests__/",
    "format:ci": "prettier --check src __tests__/",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "lint-staged --diff=main",
    "openapi:generate": "openapi --input http://localhost:8080/openapi.json --output ./src/types/api --exportCore false --exportServices false --indent 2",
    "start": "next start",
    "test": "jest --watch",
    "test:ci": "jest"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fidesui/components": "^0.2.2",
    "@fidesui/react": "^0.0.21",
    "@fontsource/inter": "^4.5.4",
    "@monaco-editor/react": "^4.4.5",
    "@reduxjs/toolkit": "^1.8.0",
    "chakra-react-select": "^3.3.7",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^1.3.1",
    "formik": "^2.2.9",
    "framer-motion": "^5",
    "i18n-iso-countries": "^7.4.0",
    "immer": "^9.0.19",
    "js-yaml": "^4.1.0",
    "lodash.debounce": "^4.0.8",
    "msw": "^1.1.0",
    "narrow-minded": "^1.1.1",
    "next": "^12.3.4",
    "next-redux-wrapper": "^7.0.5",
    "next-remove-imports": "^1.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux-persist": "^6.0.0",
    "whatwg-fetch": "^3.6.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@jest/globals": "^29.3.1",
    "@next/bundle-analyzer": "^13.2.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.3",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash.debounce": "^4.0.6",
    "@types/node": "18.15.2",
    "@types/react": "17.0.38",
    "@types/react-redux": "^7.1.24",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.55.0",
    "babel-jest": "^27.5.1",
    "cross-env": "^7.0.3",
    "cypress": "^12.7.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-only-tests": "^3.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^13.0.3",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.6.2",
    "typescript": "4.9.5"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write --list-different",
      "eslint --fix"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
