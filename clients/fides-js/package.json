{
  "name": "fides-js",
  "version": "0.0.1",
  "description": "Fides Javascript",
  "license": "Apache-2.0",
  "main": "./dist/fides.js",
  "source": "./src/fides.ts",
  "module": "./dist/fides.mjs",
  "types": "./dist/fides.d.ts",
  "files": [
    "dist/**"
  ],
  "scripts": {
    "build": "NODE_ENV=production rollup -c",
    "build:windows": "set NODE_ENV=production && rollup -c",
    "clean": "rm -rf dist .turbo node_modules",
    "docs:generate": "typedoc --out docs src/types/",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --watchAll",
    "test:ci": "jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ethyca/fides.git",
    "directory": "clients/fides-js"
  },
  "dependencies": {
    "@iabgpp/cmpapi": "^3.1.0",
    "@iabtechlabtcf/cmpapi": "^1.5.8",
    "@iabtechlabtcf/core": "^1.5.7",
    "a11y-dialog": "^7.5.2",
    "base-64": "^1.0.0",
    "preact": "^10.13.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript-cookie": "^1.0.6",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@types/base-64": "^1.0.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^18.16.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-preact": "^1.3.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "js-cookie": "^3.0.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "rollup": "^3.21.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-filesize": "^10.0.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-visualizer": "^5.12.0",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.5",
    "typedoc-plugin-markdown": "^3.17.1",
    "typescript-plugin-css-modules": "^5.0.1"
  },
  "typedocOptions": {
    "disableSources": true,
    "excludeExternals": true,
    "excludePrivate": true,
    "githubPages": false,
    "hideBreadcrumbs": true,
    "hideInPageTOC": true,
    "plugin": "typedoc-plugin-markdown",
    "readme": "none"
  }
}
