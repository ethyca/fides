{
  "$schema": "https://turbo.build/schema.v1.json",
  "globalEnv": ["IS_TEST"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/*", "!.next/cache/*", "dist/*"]
    },
    "build:windows": {
      "dependsOn": ["^build:windows"],
      "outputs": [".next/*", "!.next/cache/*", "dist/*"]
    },
    "clean": {
      "dependsOn": [],
      "cache": false
    },
    "dev": {
      "outputs": [".next/*", "!.next/cache/*"],
      "cache": false,
      "persistent": true
    },
    "dev:windows": {
      "dependsOn": ["^build:windows"],
      "outputs": [".next/*", "!.next/cache/*"],
      "cache": false,
      "persistent": true
    },
    "start": {
      "dependsOn": ["build"],
      "outputs": [".next/*", "!.next/cache/*"]
    },
    "lint": {
      "dependsOn": []
    },
    "lint:fix": {
      "dependsOn": [],
      "cache": false
    },
    "format": {
      "dependsOn": []
    },
    "format:ci": {
      "dependsOn": []
    },
    "typecheck": {
      "dependsOn": ["build"]
    },
    "test": {
      "dependsOn": ["^typecheck"],
      "cache": false,
      "persistent": true
    },
    "test:watch": {
      "dependsOn": ["^typecheck"],
      "cache": false,
      "persistent": true
    },
    "cy:open": {
      "dependsOn": []
    },
    "cy:run": {
      "dependsOn": []
    },
    "cy:start": {
      "dependsOn": []
    },
    "analyze": {
      "dependsOn": []
    },
    "analyze:browser": {
      "dependsOn": []
    },
    "analyze:server": {
      "dependsOn": []
    },
    "openapi:generate": {
      "dependsOn": []
    },
    "openapi:generate-dictionary": {
      "dependsOn": []
    },
    "storybook": {
      "dependsOn": []
    }
  }
}
