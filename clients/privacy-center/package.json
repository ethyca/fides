{
  "name": "privacy-center",
  "version": "2.13.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "dev:windows": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && eslint ./cypress/**/*.ts",
    "lint:fix": "next lint --fix && eslint --fix ./cypress/**/*.ts",
    "lint-staged:fix": "lint-staged --diff=main",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "test": "jest --watchAll",
    "test:ci": "npm run typecheck && jest",
    "clean": "rm -rf .turbo node_modules .next",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:start": "export NODE_ENV=test && export FIDES_PRIVACY_CENTER__IS_OVERLAY_ENABLED=true && npm run build && next start -p 3001",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "openapi:generate": "openapi --input http://localhost:8080/openapi.json --output ./types/api --exportCore false --exportServices false --indent 2 && prettier --write ."
  },
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fontsource/inter": "^4.5.15",
    "@iabtechlabtcf/core": "^1.5.10",
    "@reduxjs/toolkit": "^1.9.3",
    "@types/react-dom": "^18.3.0",
    "cache-control-parser": "^2.0.4",
    "fides-js": "*",
    "fidesui": "*",
    "formik": "^2.2.9",
    "framer-motion": "^11.2.12",
    "headers-polyfill": "^4.0.3",
    "immer": "^9.0.21",
    "next": "^14.2.10",
    "next-swagger-doc": "^0.4.0",
    "next-transpile-modules": "^10.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.0.13",
    "react-phone-number-input": "^3.4.1",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "swagger-ui-react": "^5.17.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.10",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "18.15.10",
    "@types/react": "^18.3.2",
    "@types/swagger-ui-react": "^4.18.0",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.15.2",
    "cypress-real-events": "^1.13.0",
    "cypress-wait-until": "^3.0.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "^15.1.7",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^3.4.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.8.3",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "node-mocks-http": "^1.15.0",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^3.3.3",
    "sass": "^1.80.6",
    "typescript": "4.9.5",
    "whatwg-fetch": "^3.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write --list-different",
      "eslint --fix"
    ]
  }
}
