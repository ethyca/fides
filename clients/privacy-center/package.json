    "formik": "^2.2.9",
    "framer-motion": "^5",
    "headers-polyfill": "^3.0.10",
    "next": "12.2.5",
    "next-auth": "^4.10.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
