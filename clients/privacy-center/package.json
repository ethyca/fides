{
  "name": "privacy-center",
  "version": "2.13.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:windows": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "lint-staged --diff=main",
    "format": "prettier --write .",
    "format:ci": "prettier --check .",
    "test": "jest --watchAll",
    "test:ci": "tsc --noEmit && jest",
    "clean": "rm -rf .turbo node_modules",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:start": "export NODE_ENV=test && npm run build && npm run start",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "openapi:generate": "openapi --input http://localhost:8080/openapi.json --output ./types/api --exportCore false --exportServices false --indent 2"
  },
  "dependencies": {
    "@chakra-ui/react": "^1.8.9",
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@fidesui/react": "^0.0.23",
    "@fontsource/inter": "^4.5.15",
    "@iabtechlabtcf/core": "^1.5.10",
    "@reduxjs/toolkit": "^1.9.3",
    "cache-control-parser": "^2.0.4",
    "fides-js": "*",
    "formik": "^2.2.9",
    "framer-motion": "^5",
    "headers-polyfill": "^3.1.2",
    "immer": "^9.0.21",
    "next": "12.2.5",
    "next-swagger-doc": "^0.3.6",
    "next-transpile-modules": "^10.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-error-boundary": "^4.0.4",
    "react-phone-number-input": "^3.2.19",
    "react-redux": "^8.0.5",
    "redux-persist": "^6.0.0",
    "swagger-ui-react": "^4.18.3",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^12.0.10",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "18.15.10",
    "@types/react": "17.0.38",
    "@types/swagger-ui-react": "^4.18.0",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "babel-jest": "^29.5.0",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.0",
    "cypress-wait-until": "^1.7.2",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsdoc": "^44.2.4",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-no-only-tests": "^3.1.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "lint-staged": "^13.2.0",
    "msw": "^1.2.1",
    "node-mocks-http": "^1.12.2",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.8.7",
    "typescript": "4.9.5",
    "whatwg-fetch": "^3.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "prettier --write --list-different",
      "eslint --fix"
    ]
  }
}
